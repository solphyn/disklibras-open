// Use this file as a starting point for your project's .eslintrc.
// Copy this file, and add rule overrides as needed.
{
  "extends": "airbnb",
  "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "script",
      "ecmaFeatures": {
        "modules": false
      }
  },
  "env": {
    "node": true,
    },
  "rules": {
    "indent": ["error", 2, { "SwitchCase": 1 }],
    "no-var": "off",
    "vars-on-top": "off",
    "no-underscore-dangle": "off",
    "prefer-template": "off",
    "no-multi-assign": "off",
    "strict": "off",
    "no-param-reassign": "off",
    "no-shadow": "off",
    "no-plusplus": "off",
    "func-names": ["error", "never"],
    "no-use-before-define": "off",
    "default-case": "off",
    "no-undef": "off",
    "no-unused-expressions": "off",
    "no-unused-vars": "off",
    "block-scoped-var": "off",
    "no-redeclare": "off",
    "no-empty": "off",
    "no-console": "off",
    "global-require": "off",
    "one-var-declaration-per-line": "off",
    "one-var": "off",
    "comma-dangle": "off",
    "new-cap": "off",
    
  }
}
